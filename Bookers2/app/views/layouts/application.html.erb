<!DOCTYPE html>
<html>
<% if user_signed_in? %>
      <li class="nav nav-tabs">
        <%= link_to "ホーム", books_path, class:"home" %>
        <%= link_to "ログアウト", destroy_user_session_path,class:"logout", method: :delete %>
        <%= link_to 'Book', books_path,class:"books" %>
        <%= link_to 'Users', users_path,class:"users" %>
      </li>
    <% else %>
      <li class="nav nav-tabs">
        <%= link_to "サインアップ", new_user_registration_path %>
        <%= link_to "ログイン", new_user_session_path,class:"sign_in" %>
        <%= link_to "ホーム", "#{}",class:"home" %>
        <%= link_to "アバウト",home_about_path,class:"about" %>
      </li>
    <% end %>
  <head>
    <title>Bookers2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
